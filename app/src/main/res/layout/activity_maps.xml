<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ProgressBar
        android:id="@+id/mapsActivity_pb_webviewProgressbar"
        style="@android:style/Widget.DeviceDefault.Light.ProgressBar.Inverse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        app:layout_constraintLeft_toLeftOf="@+id/mapsActivity_wv_kaartBreda"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/mapsActivity_wv_kaartBreda"
        app:layout_constraintTop_toTopOf="@+id/mapsActivity_wv_kaartBreda"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/mapsActivity_wv_kaartBreda"
        android:layout_marginBottom="8dp" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/activityMain_Fbtn_gps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:clickable="true"
        app:backgroundTint="@color/mooiGroen"
        app:fabSize="normal"
        app:srcCompat="@drawable/ic_gps_fixed_white_48dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="@+id/mapsActivity_wv_kaartBreda"
        app:layout_constraintBottom_toBottomOf="@+id/mapsActivity_wv_kaartBreda"
        android:layout_marginBottom="8dp" />

    <ImageView
        android:id="@+id/activityMaps_iv_help"
        android:layout_width="45dp"
        android:layout_height="31dp"
        android:layout_x="79dp"
        android:layout_y="439dp"
        app:srcCompat="@drawable/help_circle_outline"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteY="472dp" />

    <WebView
        android:id="@+id/mapsActivity_wv_kaartBreda"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/activityMaps_tv_help">


    </WebView>

    <TextView
        android:id="@+id/activityMaps_tv_help"
        android:layout_width="0dp"
        android:textColor="@color/white"
        android:layout_height="wrap_content"
        android:text="@string/activityMaps_tv_selectLocationHelp"
        app:layout_constraintLeft_toRightOf="@+id/activityMaps_iv_help"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/activityMaps_iv_help"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/activityMaps_iv_help"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>

